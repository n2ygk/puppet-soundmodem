<?xml version="1.0"?>
<modem>
  <configuration name="<%= @intf -%>">
    <chaccess txdelay="150" slottime="100" ppersist="40" fulldup="0" txtail="10"/>
    <audio type="alsa" device="plughw:0,0" halfdup="0" capturechannelmode="Mono"/>
    <ptt file="<%= @pttdev -%>"/>
    <channel name="Channel 0">
      <mod mode="afsk" bps="1200" f0="1200" f1="2200" diffenc="1"/>
      <demod mode="afsk" bps="1200" f0="1200" f1="2200" diffdec="1"/>
      <pkt mode="MKISS" ifname="<%= @intf -%>" hwaddr="<%= @call -%>" ip="10.0.0.1" netmask="255.255.255.0" broadcast="10.0.0.255"/>
    </channel>
  </configuration>
</modem>
